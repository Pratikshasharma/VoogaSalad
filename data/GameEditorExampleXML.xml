<root>
	<level value="one" title="flappybird">
		<gobject type="mainchar">
			<position israndom="false" xcor="100" ycor="50"/>
			<image file="bird.jpg"/>
			<behavior input="spacebar" command="up" method="step"/>
			<property health="1" destructible="true" static="false"/>
		</gobject>
		<gobject type="flappybirdpipe">
			<position israndom="true" spacing="250" minheight="100" maxheight="500"/>
			<image file="greenpipe.jpg"/>
			<property damage="1" points="1" destructible="false" static="true"/>
		</gobject>
		<rules>
			<scrolltype value="forced" direction="right"/>
			<score condition="objectivebased"/>
			<time limit="infinite"/>
			<boundaries bottom="die"/>
		</rules>
		<settings>
			<music file="payphone.mp3"/>
			<image file="mountains.jpg"/>
		</settings>
	</level>
</root>
